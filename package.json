  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:dev": "jest",
    "test2": "firebase emulators:exec --ui --import=src/firebase/emulator-data/ 'npm run jest'",
    "test": "react-scripts test",
    "test:rules": "react-scripts test --config=firebase.jest.config.js --detectOpenHandles --forceExit",
    "test:coverage": "npm run test:dev -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .jsx,.js,.ts,.tsx",
    "lint:fix": "eslint src --fix",
    "extract-messages": "formatjs extract src/**/*.js* --out-file src/i18n/en.json",
    "deploy": "npm run build && firebase deploy",
    "jest": "react-scripts test --runInBand",
    "mock": "firebase emulators: start",
    "firebase": "firebase emulators:exec --import=src/firebase/emulator-data/",
    "pre-push": "npm run test:dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pre-commit && lint-staged",
      "pre-push": "./commands/pre-push $HUSKY_GIT_STDIN"
    }
  },
  "lint-staged": {
    "*": "eslint src .jsx,.js,.ts,.tsx --fix"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@firebase/testing": "^0.20.11",
    "@testing-library/dom": "^8.11.3",
